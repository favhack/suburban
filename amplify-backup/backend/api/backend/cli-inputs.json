{
  "version": 1,
  "paths": {
    "/image/{imageId}": {
      "name": "/image/{imageId}",
      "lambdaFunction": "imageSync",
      "permissions": {
        "setting": "open"
      }
    },
    "/group": {
      "name": "/group",
      "lambdaFunction": "createGroup",
      "permissions": {
        "setting": "open"
      }
    },
    "/group/{groupId}/member": {
      "name": "/group/{groupId}/member",
      "lambdaFunction": "createGroup",
      "permissions": {
        "setting": "open"
      }
    }
  }
}